{
	"manifest_version": 3,
	"name": "Yellow You NewTab",
	"version": "3.0.29",
	"description": "A Simple New Tab (browser's home page) inspired by Google's 'Yellow You' design.",
	"optional_permissions": ["bookmarks"],
	"host_permissions": [
		"https://www.google.com/complete/search*",
		"https://duckduckgo.com/ac/*",
		"https://search.brave.com/api/suggest*"
	],
	"icons": {
		"16": "favicon/icon16.png",
		"48": "favicon/icon48.png",
		"128": "favicon/icon128.png"
	},
	"action": {
		"default_icon": "favicon/icon48.png"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	}
}
